<Window x:Class="Parser.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Parser"
        mc:Ignorable="d"
        Title="MainWindow" Height="441.608" Width="638.892">
    <Grid RenderTransformOrigin="0.5,0.5" Margin="0,-1,1,3">
        <Grid.RenderTransform>
            <TransformGroup>
                <ScaleTransform/>
                <SkewTransform/>
                <RotateTransform Angle="-0.146"/>
                <TranslateTransform/>
            </TransformGroup>
        </Grid.RenderTransform>
        <TextBox x:Name="EnterSite" HorizontalAlignment="Left" Height="21" Margin="71,58,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="502" Text="http://www.cyberforum.ru/"/>
        <Button x:Name="Button1" Content="Отобразить имена пользователей" HorizontalAlignment="Left" Height="32" Margin="171,105,0,0" VerticalAlignment="Top" Width="279" Click="Button_Click"/>
        <TextBox x:Name="Logins" HorizontalAlignment="Left" Height="227" Margin="259,158,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="118"/>
    </Grid>
</Window>
